<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="shop.ProductsEntity" table="PRODUCTS" schema="SYSTEM">
        <id name="productId">
            <column name="PRODUCT_ID" sql-type="number(*)" precision="2147483646"/>
        </id>
        <property name="prize">
            <column name="PRIZE" sql-type="number(*)" precision="2147483646"/>
        </property>
        <property name="productAvailability">
            <column name="PRODUCT_AVAILABILITY" sql-type="number(*)" precision="2147483646"/>
        </property>
        <property name="name">
            <column name="NAME" sql-type="varchar2(20)" length="20"/>
        </property>
        <many-to-one name="characteristicsByCharacteristicId" class="shop.CharacteristicsEntity">
            <column name="CHARACTERISTIC_ID"/>
        </many-to-one>
        <many-to-one name="categoriesByCategoryId" class="shop.CategoriesEntity">
            <column name="CATEGORY_ID"/>
        </many-to-one>
        <many-to-one name="bookingsByBookingId" class="shop.BookingsEntity">
            <column name="BOOKING_ID"/>
        </many-to-one>
    </class>
</hibernate-mapping>