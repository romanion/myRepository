<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="base.BookingsEntity" table="BOOKINGS" schema="public" catalog="postgres">
        <id name="bookingId">
            <column name="BOOKING_ID" sql-type="numeric" precision="-1"/>
        </id>
        <property name="sum">
            <column name="SUM" sql-type="numeric" precision="-1" not-null="true"/>
        </property>

        <property name="data">
            <column name="DATA" sql-type="date" not-null="true"/>
        </property>

        <many-to-one name="productsByProductId" class="base.ProductsEntity">
            <column name="PRODUCT_ID"/>
        </many-to-one>
        <many-to-one name="customersByCustomerId" class="base.CustomersEntity">
    <column name="CUSTOMER_ID"/>
</many-to-one>
</class>

</hibernate-mapping>