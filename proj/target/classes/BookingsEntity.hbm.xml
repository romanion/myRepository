<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="shop.BookingsEntity" table="BOOKINGS" schema="SYSTEM">
        <id name="bookingId">
            <column name="BOOKING_ID" sql-type="number(*)" precision="2147483646"/>
        </id>
        <property name="sum">
            <column name="SUM" sql-type="number(*)" precision="2147483646"/>
        </property>
        <property name="data">
            <column name="DATA" sql-type="date"/>
        </property>
        <property name="productId">
            <column name="PRODUCT_ID" sql-type="number(*)" precision="2147483646"/>
        </property>
    </class>
</hibernate-mapping>